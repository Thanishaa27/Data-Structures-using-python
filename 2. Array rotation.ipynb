   "source": [
    "#2. Array rotation\n",
    "def rotate_array(arr, k):\n",
    "    k = k % len(arr) \n",
    "    return arr[-k:] + arr[:-k]\n",
    "\n",
    "arr = [1, 2, 3, 4, 5]\n",
    "print(rotate_array(arr, 2))"
   ]
  }
